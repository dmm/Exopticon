<h2>Show File</h2>

<ul>

  <li>
    <strong>Id:</strong>
    <span id="fileId"><%= @file.id %></span>
  </li>
  <li>
    <strong>Filename:</strong>
    <%= @file.filename %>
  </li>

  <li>
    <strong>Size:</strong>
    <%= @file.size %> (<%= @file.size / 1024 / 1024 %> MiB)
  </li>

  <li>
    <strong>Begin time:</strong>
    <%= Enum.at(@file.time, 0) %>
  </li>

  <li>
    <strong>End time:</strong>
    <%= Enum.at(@file.time, 1) %>
  </li>

  <li>
    <strong>Begin monotonic:</strong>
    <%= @file.begin_monotonic %>
  </li>

  <li>
    <strong>End monotonic:</strong>
    <%= @file.end_monotonic %>
  </li>

  <li>
    <strong>Monotonic index:</strong>
    <%= @file.monotonic_index %>
  </li>

</ul>

<span><%= link "Edit", to: file_path(@conn, :edit, @file) %></span>
<span><%= link "Back", to: file_path(@conn, :index) %></span>
<div class="video"></div>
